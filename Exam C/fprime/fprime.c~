#include <stdio.h>

void	my_putchar(char c)
{
  write(1, &c, 1);
}

void	fprime(int nb)
{
  int	i;
  int	first;

  first = 0;
  i = 0;
  while (i < nb)
    {
      if (first == 1)
	printf("*");
      i = 2;
      while (nb % i != 0)
	i++;
      printf("%d", i);
      nb /= i;
      first = 1;
    }
  if (first == 0)
    printf("%d", nb);
  printf("\n");
}

int	main(int ac, char **av)
{
  if (ac == 1)
    my_putchar(10);
  else
    fprime(atoi(av[1]));
  return (0);
}
