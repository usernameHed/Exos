SRC_H=	my.h

EXEC= libmy.a

OBJ=	$(SRC:.c=.o)

all:	compil copy_c copy_h

compil:	$(OBJ)
	ar rc $(EXEC)

copy_c:
	cp $(EXEC) ../

copy_h:
	cp $(SRC_H) ../../include/

clean:
	rm -f $(OBJ)
fclean:		clean
	rm -f $(NAME)

re: fclean all
